{
  "name": "my-app-template",
  "version": "1.0.0",
  "description": "Template for a Node/React web application with Webpack and TypeScript",
  "main": "index.ts",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "start": "ts-node src/server/index.ts",
    "local-start:server": "nodemon src/server/index.ts",
    "local-start:client": "webpack serve",
    "local-start": "concurrently \"npm run local-start:server\" \"npm run local-start:client\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/franciscocgue/my-app-template.git"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "keywords": [
    "React",
    "Node",
    "Webpack",
    "TypeScript",
    "web",
    "application"
  ],
  "author": "Francisco Carmona Guerrero",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/franciscocgue/my-app-template/issues"
  },
  "homepage": "https://github.com/franciscocgue/my-app-template#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mongodb": "^4.11.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.4.2",
    "socket.io": "^4.5.3",
    "socket.io-client": "^4.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.38",
    "@types/react": "^18.0.10",
    "@types/react-dom": "^18.0.5",
    "@types/socket.io": "^3.0.2",
    "babel-jest": "^28.1.2",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.3.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.2",
    "mini-css-extract-plugin": "^2.6.0",
    "nodemon": "^2.0.19",
    "react-test-renderer": "^18.2.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "typescript-plugin-css-modules": "^3.4.0",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  }
}
